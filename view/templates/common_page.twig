	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">        
	<link rel="icon" type="image/png" href="../../public/img/favicon-insight-signals.png" />

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="../../public/assets/css/bootstrap/bootstrap.min.css">
    <link href="../../public/img/open-iconic-master/font/css/open-iconic-bootstrap.css" rel="stylesheet">
	<link href="../../public/assets/css/simple-sidebar.css" rel="stylesheet">
	<link href="../../public/assets/css/custom.css" rel="stylesheet">
    <script src="../../public/assets/js/jquery.min.js"></script>
